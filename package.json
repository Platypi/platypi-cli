{
  "name": "platypi-cli",
  "version": "0.11.3",
  "description": "A command line interface for working with PlatypusTS projects.",
  "author": "Platypi <support@platypi.io>",
  "homepage": "https://platypi.io",
  "main": "src/index.js",
  "preferGlobal": true,
  "bin": {
    "plat": "bin/platypi"
  },
  "engines": {
    "node": ">= 0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Platypi/platypi-cli.git"
  },
  "keywords": [
    "platypi",
    "platypusts",
    "typescript",
    "framework"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Platypi/platypi-cli/issues"
  },
  "dependencies": {
    "chalk": "^1.1.0",
    "configstore": "^1.2.1",
    "es6-promise": "^3.0.2",
    "exit": "^0.1.2",
    "findup": "^0.1.5",
    "fs-extra": "^0.26.4",
    "glob": "^6.0.4",
    "handlebars": "^4.0.5",
    "inquirer": "^0.11.2",
    "json-stable-stringify": "^1.0.0",
    "lodash": "^3.10.1",
    "minimist": "^1.1.2",
    "mkdirp": "^0.5.1",
    "node-uuid": "^1.4.3",
    "pleasant-progress": "^1.1.0",
    "require-all": "^2.0.0",
    "resolve": "^1.1.6",
    "rimraf": "^2.4.2",
    "swag": "^0.7.0",
    "through": "^2.3.8",
    "validate-npm-package-name": "^2.2.2"
  },
  "devDependencies": {
    "chai": "latest",
    "chai-as-promised": "latest",
    "istanbul": "latest",
    "mocha": "latest",
    "sinon": "latest",
    "sinon-chai": "latest",
    "tsconfig-glob": "latest",
    "tsconfig-lint": "latest",
    "typings": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "prepublish": "npm run typings && npm run build",
    "build": "tsconfig && tsc -p .",
    "clean": "rimraf src/**/*.js src/*.js test/unit/**/*.js test/*.js",
    "cli": "npm run build && node bin/platypi",
    "lint": "tsconfig-lint -u",
    "test": "npm run build && istanbul cover --dir test/coverage -- test/runner.js",
    "tsconfig": "tsconfig",
    "typings": "typings install"
  },
  "typescript": {
    "definition": "src/cli.d.ts"
  }
}
